<% provide(:title, "View user")%>
<h1>User Details</h1>
<div class="block">
<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Created:</strong>
  <%= @user.created_at.strftime("%b %d, %Y %H:%M:%S")  %>
</p>
</div>
<%= link_to 'Edit', edit_user_path(@user) %>
<% if session[:is_admin] %> | <%= link_to 'Back', users_path %> <% end %>
